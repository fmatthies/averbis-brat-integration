      "uniqueId": "clinical_sections_de:59776-5"
    },
    {
      "begin": 1630,
      "end": 1737,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "Ultraschall der hirnversorgenden Arterien: Arteriosklerotische Veränderungen i.B.d. Bifurkation, sonst o.B.",
      "id": 71086,
      "matchedTerm": "Ultraschall",
      "dictCanon": "Sonographie",
