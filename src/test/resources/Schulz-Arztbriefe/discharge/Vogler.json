    },
    {
      "begin": 1323,
      "end": 1408,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "Transthorakales und transösophageale Echokardiographie: keine kardiale Emboliequelle.",
      "id": 71064,
      "matchedTerm": "Transösophageale Echokardiographie",
