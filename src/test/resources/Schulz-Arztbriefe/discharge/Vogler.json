      "conceptId": "29545-1",
      "source": "clinical_sections_de",
      "label": "FindingSection",
      "uniqueId": "clinical_sections_de:29545-1"
    },
    {
      "begin": 946,
      "end": 1084,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "Internistischer Befund: \n80-jähriger Patient in gutem AZ und adipösem EZ, RR 140/80 mmHg, Herzfrequenz 80/Minute. Cor, Pulmo, Abdomen o.B.",
      "id": 71020,
      "matchedTerm": "Befund",
