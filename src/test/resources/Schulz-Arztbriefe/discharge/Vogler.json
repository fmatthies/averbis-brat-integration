    {
      "begin": 1108,
      "end": 1220,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "EKG: \nFrequenz 84/Minute, rhythmisch, Indifferenztyp, keine Erregungsausbreitungs- oder – rückbildungsstörungen.",
      "id": 71042,
      "matchedTerm": "EKG",
