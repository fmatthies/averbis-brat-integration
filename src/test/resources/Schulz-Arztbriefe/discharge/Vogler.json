    {
      "begin": 214,
      "end": 333,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "Diagnosen: \nTransitorisch-ischämische Attacke \nV.a. cerebrale Microangiopathie\nArterielle Hypertonie \nDiabetes mellitus",
      "id": 70976,
      "matchedTerm": "Diagnosen",
      "dictCanon": "Diagnose",
