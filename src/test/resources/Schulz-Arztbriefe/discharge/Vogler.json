    },
    {
      "begin": 888,
      "end": 943,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "Psychischer Befund: \nWach, voll orientiert, kooperativ.",
      "id": 71009,
      "matchedTerm": "Befund",
