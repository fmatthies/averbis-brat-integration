      "begin": 2126,
      "end": 2226,
      "type": "de.averbis.types.health.ClinicalSection",
      "coveredText": "Farbdoppler-Echokardiographie: LA mit 45mm vergrößert u. LV mit EDD 58 u. ESD 33mm grenzwertig groß.",
      "id": 121894,
      "matchedTerm": "Echokardiographie",
      "dictCanon": "Echokardiographie",
      "conceptId": "Sec_Echocardiography",
